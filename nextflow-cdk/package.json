{
  "name": "cdk-typescript",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "rimraf '**/*.js' '**/*.d.ts'",
    "deploy": "npm run build && cdk deploy --all",
    "deploy:core": "npm run build && cdk deploy -a 'node bin/core/app.js'",
    "deploy:permissions": "npm run build && cdk deploy -a 'node bin/permission/app.js'",
    "deploy:image-builder": "npm run build && cdk deploy -a 'node bin/image-builder/app.js'",
    "synth": "npm run build && cdk synth",
    "diff": "npm run build && cdk diff"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "aws-cdk": "^2.1100.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.4.2",
    "minimatch": ">=3.0.5",
    "prettier": "2.3.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^10.2.0",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "@aws-cdk/aws-codestar-alpha": "2.165.0-alpha.0",
    "aws-cdk-lib": "^2.138.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.4.5",
    "minimatch": ">=3.0.5",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.20",
    "vm2": ">=3.9.11"
  }
}
